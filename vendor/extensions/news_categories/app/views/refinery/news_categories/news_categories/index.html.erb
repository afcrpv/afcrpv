<% content_for :body do %>
  <ul id="news_categories">
    <% @news_categories.each do |news_category| %>
      <li>
        <%= link_to news_category.title, refinery.news_categories_news_category_path(news_category) %>
      </li>
    <% end %>
  </ul>
<% end %>

<%= render '/refinery/content_page' %>
