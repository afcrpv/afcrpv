<% content_for :body_content_title do %>
  <%= @agence.name %>
<% end %>

<% content_for :body do %>
  <section>
    <h2>
      <%=raw @agence.description %>
    </h2>
  </section>
  <hr>
  <section>
    <% cache @agence.name, expires_in: 30.seconds do %>
      <%= get_agence_feeds(@agence, -1) %>
    <% end -%>
  </section>
<% end %>

<%= render '/refinery/content_page' %>
