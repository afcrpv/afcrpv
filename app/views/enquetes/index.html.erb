<div id="body_content_title">
  <h1>Liste des enquêtes</h1>
</div>
<div id="body">
  <% if @enquetes.any? %>
    <ul id="enquetes">
      <% @enquetes.each do |enquete| %>
        <li>
          <%= link_to enquete.code_bnpv, enquete %>
        </li>
      <% end %>
    </ul>
  <% else %>
    Aucune enquête.
  <% end -%>
</div>
